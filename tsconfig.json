{
  "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "dist",
      "allowJs": true,
      "alwaysStrict": true,
      "declaration": true,
      "sourceMap": false,
      "noImplicitAny": false,
      "resolveJsonModule": true,
      "downlevelIteration": true,
      "esModuleInterop": true,
      "lib": [
          "es5",
          "DOM",
      ]
      
  },
  "include": [
      "src/*.ts",
  ],
  "exclude": []
}